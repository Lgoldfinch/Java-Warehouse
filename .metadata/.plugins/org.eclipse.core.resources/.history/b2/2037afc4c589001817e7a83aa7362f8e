package level2;

public class Blackjack {

	public int numbers(int a, int b) {
		if (a == b && a < 22) {
			return a;}
			else if (a == b && a > 22)
				return 0;
			else if (a > b && a < 22)
				return a;
			else if (a < b && b < 22)
				return b;
			else if (a > 21 || b > 21 )
				return 0;
	} return 0;
}
}
