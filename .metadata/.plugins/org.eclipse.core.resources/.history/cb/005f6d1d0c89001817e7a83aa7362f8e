package level1;

public class q2 {
  public static String hello(String hHello) {
	return hHello;
	  
  }
	  public static String party(String cake) {
		  return cake;
  }
	  public static int tissue(int red, int blue) {
		  return red + blue;
	  }
	  public static String retuurn(String pen) {
	  return pen; 
	  }
	  
	  
	  
     public int tissue2(int red, int blue, boolean green) {
    	 
    	 if (green) {
    		 return blue + red;
    	 }else {
    		 return blue * red;
    	 }
	 
	 
     }
}
